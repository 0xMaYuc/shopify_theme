  <div class="btn-wrapper center">
            <a href="{{section.settings.btnlink | url}}" class="Button Button--primary">
              {{section.settings.btntext}}
            </a>
        </div>
  
    </div>

</section>




<script>
  document.addEventListener("DOMContentLoaded", function(event) {
   /*
    jQuery('.faq-section > .faq-section--title-wrapper').click(function() {
      jQuery(this).parent().toggleClass('open');
    });
*/
    jQuery('.faq-section--item-title').click(function() {
      jQuery(this).parent().toggleClass('open');
    });
  });
</script>


