
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>

<section {% if section.settings.background %}style="background:{{ section.settings.background}};"{% endif %} id="section-{{ section.id }}" data-section-id="{{ section.id }}" data-section-type="transparency-questions">

<div class="faq">
      <h1 class="page-title">{{section.settings.headline}}</h1>
  	  <p style="text-align:center;">{{section.settings.subline}}</p>
      <div id="tab-faq-1" class="faq-section open">
        <div class="faq-section--title-wrapper"><h2 class="faq-section--title">{{section.settings.faqHeadline}}</h2>
        </div>
        <ul>
          <li>
