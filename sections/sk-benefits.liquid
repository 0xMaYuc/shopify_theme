{
  "name": "SK - Benefits",
  "settings": [
    {
      "type": "color",
      "id": "background",
      "label": "Background Color",
      "default": "#ffffff"
    },
    {
      "type": "text",
      "id": "headline",
      "label": "Headline",
      "default": "Headline"
    },
	{
      "type": "text",
      "id": "subline",
      "label": "Subline",
      "default": "Subline"
    },
	{
      "type": "text",
      "id": "pictext",
      "label": "Text",
      "default": "Text"
    },
    {
        "type": "image_picker",
        "id": "imagesupport1",
        "label": "Bild1",
        "info": "120 x 120px .jpg recommended"
      },
      {
        "type": "text",
        "id": "headline1",
        "label": "Headline1",
        "default": "Headline1"
      },
      {
        "type": "text",
        "id": "text1",
        "label": "text1",
        "default": "text1"
      },
	{
      "type": "text",
      "id": "btntext",
      "label": "Button Text",
      "default": "Text"
    },
	{
      "type": "text",
      "id": "helplink",
      "label": "Link URL",
      "default": "mailto:hallo@fundw.net"
    }
  ],
  "presets": [{
    "name": "SK - Benefits",
    "category": "Custom"
  }]
}
